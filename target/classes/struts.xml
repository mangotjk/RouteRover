<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <constant name="struts.devMode" value="true" />

    <package name="routerover" extends="struts-default">
        <action name="index">
            <result>/index.jsp</result>
        </action>

        <action name="hello" class="authentication.HelloWorldAction" method="execute">
            <result name="success">/HelloWorld.jsp</result>
        </action>

        <action name="Login" class="authentication.Login" >
            <result name="success">welcome.jsp</result>
            <result name="input">index.jsp</result>
        </action>

        <action name="map" class="authentication.map" method="execute">
            <result name="success">/map.jsp</result>
        </action>

        <action name="mapSearch" class="authentication.mapSearch" method="execute">
            <result>/mapSearch.jsp</result>
        </action>

        <action name="myruns" class="myRuns.MyRuns" method="execute">
            <result>/myRuns.jsp</result>
        </action>

    </package>

</struts>